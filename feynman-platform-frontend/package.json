{
  "name": "feynman-platform-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "d3-force-3d": "^3.0.0",
    "three": "^0.172.0",
    "dompurify": "^3.3.0",
    "i18next": "^25.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.2.2",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.9.4",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^5.4.10",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^31.6.0",
    "electron-builder": "^25.1.8",
    "wait-on": "^7.2.0",
    "vite-plugin-pwa": "^0.20.5"
  },
  "main": "electron.js",
  "build": {
    "appId": "com.feynman-platform.app",
    "productName": "Feynman Learning Platform",
    "files": [
      "dist/**/*",
      "electron.js"
    ],
    "directories": {
      "output": "release"
    }
  }
}
